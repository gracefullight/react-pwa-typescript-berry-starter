{
  "name": "react-pwa-typescript-berry-starter",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gracefullight/react-pwa-typescript-berry-starter.git"
  },
  "author": {
    "name": "gracefullight",
    "email": "gracefullight.dev@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^5.0.0-beta.4",
    "@material-ui/icons": "^5.0.0-beta.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "clsx": "^1.1.1",
    "http-proxy-middleware": "^2.0.1",
    "jotai": "^1.3.0",
    "leaflet": "^1.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-leaflet": "^3.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "recoil": "^0.4.0",
    "web-vitals": "^2.1.0",
    "workbox-background-sync": "^6.2.4",
    "workbox-broadcast-update": "^6.2.4",
    "workbox-cacheable-response": "^6.2.4",
    "workbox-core": "^6.2.4",
    "workbox-expiration": "^6.2.4",
    "workbox-google-analytics": "^6.2.4",
    "workbox-navigation-preload": "^6.2.4",
    "workbox-precaching": "^6.2.4",
    "workbox-range-requests": "^6.2.4",
    "workbox-routing": "^6.2.4",
    "workbox-strategies": "^6.2.4",
    "workbox-streams": "^6.2.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "airbnb-typescript"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "settings": {
      "import/resolver": {
        "typescript": {}
      }
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/eslint": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/leaflet": "^1.7.6",
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "devmoji": "^2.3.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "pinst": "^2.1.6",
    "prettier": "^2.5.0",
    "sass": "^1.43.5",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.3.5"
  },
  "packageManager": "yarn@3.1.1"
}
